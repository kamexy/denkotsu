{
  "name": "denkotsu",
  "version": "0.12.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "build:turbopack": "next build",
    "start": "next start",
    "lint": "eslint",
    "check:data": "node scripts/validate-content.mjs",
    "check:monetization": "node scripts/validate-monetization-env.mjs",
    "check:data:ci": "node scripts/validate-content.mjs --fail-on-warn --baseline scripts/validate-content-warning-baseline.json",
    "check:data:update-baseline": "node scripts/validate-content.mjs --write-baseline scripts/validate-content-warning-baseline.json",
    "check": "npm run lint && npm run check:data && npm run check:monetization && npm run build",
    "release:patch": "npm version patch -m \"chore(release): v%s\"",
    "release:minor": "npm version minor -m \"chore(release): v%s\"",
    "release:major": "npm version major -m \"chore(release): v%s\""
  },
  "dependencies": {
    "dexie": "^4.3.0",
    "framer-motion": "^12.34.0",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
